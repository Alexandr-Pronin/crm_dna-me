{
  "name": "dna-marketing-engine",
  "version": "1.0.0",
  "description": "DNA ME - Custom CRM Platform with Smart Routing & Intent Detection",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "workers": "tsx src/workers/index.ts",
    "dev:workers": "tsx watch src/workers/index.ts",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "seed": "tsx src/db/seeds/run.ts",
    "test": "vitest",
    "test:e2e": "tsx scripts/test-e2e.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "crm",
    "marketing",
    "automation",
    "lead-scoring",
    "intent-detection"
  ],
  "author": "DNA ME",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "axios": "^1.6.7",
    "bullmq": "^5.1.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.1",
    "ioredis": "^5.3.2",
    "node-pg-migrate": "^7.0.0",
    "pg": "^8.11.3",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
